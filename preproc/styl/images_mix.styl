create_images()
    .images_block
        display flex
        flex-direction row
        width 100%
        text-align center
        margin 15px 0
        &-item
            width 33.33%
        img
            width 95%
    @media(max-width:725px)
        .images_block
            flex-direction column
            align-items center
            &-item
                width 100%
                padding 10px